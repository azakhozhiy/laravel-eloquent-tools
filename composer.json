{
  "name": "azakhozhiy/laravel-eloquent-tools",
  "version": "1.0.0",
  "description": "Laravel Eloquent Tools",
  "authors": [
    {
      "name": "Aleksandr Zakhozhiy",
      "email": "azakhozhiiy@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "laravel/framework": "^10.48",
    "tucker-eric/eloquentfilter": "^3.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "orchestra/testbench": "^8.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "AZakhozhiy\\Laravel\\Eloquent\\Tools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AZakhozhiy\\Laravel\\Eloquent\\Tools\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "csf": [
      "./vendor/bin/php-cs-fixer fix src --using-cache=no --allow-risky=yes"
    ],
    "clb": [
      "git branch | grep -v $(git branch --show-current) | grep -v \"master\" | xargs git branch -D"
    ]
  },
  "minimum-stability": "stable",
  "type": "library"
}
